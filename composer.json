{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
	"repositories": [
	{
	    "type": "package",
	    "package": {
	        "name": "greenrobotllc/laravel-resourceful",
	        "version": "dev-master",
	        "source": {
	            "url": "https://github.com/greenrobotllc/laravel-resourceful.git",
	            "type": "git",
	            "reference": "master"
	        }
	    }
	}],
    "require": {
        "php": ">=7.1.3",
        "aws/aws-sdk-php-laravel": "~3.0",
        "cartalyst/stripe-laravel": "11.0.*",
        "jenssegers/agent": "^2.5",
        "laracasts/generators": "dev-master as 1.1.3",
        "laravel/framework": "6.*.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^6.0.3",
        "laravelista/ekko": "^3.2",
        "league/flysystem-aws-s3-v3": "^1.0",
        "sebastian/diff": "^3.0",
        "torann/geoip": "^1.0",
        "wangaz/laravel-semantic-ui-css": "1.3.0",
        "webpatser/laravel-uuid": "^2.0",
        "ext-intl": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "greenrobotllc/laravel-resourceful": "dev-master",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
        "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
        "@php artisan package:discover"
        ],

        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "mkdir -p public/assets",
            "ln -sf ../../vendor/wangaz/laravel-semantic-ui-css/src/assets/jquery public/assets/jquery",
            "ln -sf ../../vendor/wangaz/laravel-semantic-ui-css/src/assets/semantic-ui public/assets/semantic-ui"

        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
